{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/.well-known/*",
      "/assets/*",
      "/*.{ico,css,js,json}"
    ]
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, OPTIONS",
    "Access-Control-Allow-Headers": "*"
  }
}
